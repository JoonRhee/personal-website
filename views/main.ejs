  
<!DOCTYPE html>
<html lang="en" prefix="og: https://ogp.me/ns#">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta property="og:title" content="<%= title %>" /> 
    <meta name="image" property="og:image" content="<%= img %>" /> 
    <meta name="author" content="<%= author %>" /> 
    <meta property="og:description" content="<%= description %>" /> 
    <meta property="og:url" content="<%= URL %>" />

    <title><%= title %></title>
    <link rel="icon" href="<%= icon %> ">

    <!-- Google Font -->

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Noto+Sans&family=Roboto:wght@900&display=swap" rel="stylesheet">

    <!-- animation and jquery -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"/>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>


    <!-- Fontawsome -->
    <script src="https://kit.fontawesome.com/49afc30d47.js" crossorigin="anonymous"></script>

    <link rel='stylesheet' type='text/css' href='/stylesheets/main.css'/>

    <script>
        var isIE = false || !!document.documentMode;

        function imgOnLoad(id){
            let elemClass = document.getElementById(id).classList;
            elemClass.add("img-loaded");
        }
    </script>

</head>

<body>
    <%- include('nav') %>
    <%- include('header') %>
    <%- include('about') %>
    <%- include('edu') %>
    <%- include('exp') %>
    <%- include('proj') %>
    <%- include('photo') %>
    <%- include('contact') %>
</body>